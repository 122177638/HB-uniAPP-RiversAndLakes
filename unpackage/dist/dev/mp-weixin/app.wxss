
page {
  min-height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.content {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  width: 100%;
  font-size: 28rpx;
  color: #333333;
  font-family: -apple-system, BlinkMacSystemFont, "PingFang SC","Helvetica Neue",STHeiti,"Microsoft Yahei",Tahoma,Simsun,sans-serif;
  background-color: #F6F6F6;
}
.ellipsis {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}